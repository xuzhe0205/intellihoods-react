pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh '''#!/bin/bash
                    echo 'Building..'
                    
                '''
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Push'){
            steps {
                echo 'Deploying....'
                sh '''#!/bin/bash
                    
                '''
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                sh '''#!/bin/bash
                    
                '''
            }
        }
    }
}